{
  "name": "streaming-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && node server.js",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build",
    "start:production": "concurrently \"npm run start:frontend:production\" \"npm run start:backend:production\"",
    "start:frontend:production": "npx serve -s dist -l 3000",
    "start:backend:production": "cd backend && NODE_ENV=production node server.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.7",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.15.0",
    "hls.js": "^1.6.7",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.344.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.35",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "tailwindcss": "^3.4.1",
    "zod": "^3.25.36"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "vite": "^5.4.2"
  },
  "main": "eslint.config.js",
  "keywords": [],
